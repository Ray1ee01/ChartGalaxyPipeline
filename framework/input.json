      "icon_mark": "none",
      "icon_label": "none",
      "data_label": "none",
      "data_order": "ascending",
      "has_x_axis": true,
      "has_y_axis": true
    },
    "variables": {
      "_comment": "这些属性用于指导图表的变量样式。图表中需要考虑这些属性的变化，根据变量来生成图表",
      "height": 600,
      "width": 600,
      "mark": {
          "has_rounded_corners": true,
          "has_shadow": true,
          "has_spacing": true,
